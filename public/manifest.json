{
  "name": "Smarter Logic Web.com - Solutions Web Glamour",
  "short_name": "SLW",
  "description": "Agence SLW: sites vitrines statiques, landing pages, intégrations web sur-mesure.",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#0b0b0b",
  "theme_color": "#C73863",
  "orientation": "portrait-primary",
  "scope": "/",
  "lang": "fr",
  "dir": "ltr",
  "categories": ["business", "productivity", "utilities"],
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/maskable-icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Nouveau Projet",
      "short_name": "Contact",
      "description": "Démarrer un nouveau projet web",
      "url": "/contact"
    },
    {
      "name": "Portfolio",
      "short_name": "Projets",
      "description": "Voir nos réalisations",
      "url": "/portfolio"
    },
    {
      "name": "Services",
      "short_name": "Offres",
      "description": "Découvrir nos services",
      "url": "/services"
    }
  ],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "mailto",
      "url": "/contact?email=%s"
    }
  ],
  "related_applications": [
    {
      "platform": "webapp",
      "url": "https://smarterlogicweb.com/manifest.json"
    }
  ]
